EXE = .exe
INSTALLROOTDIR = $(HOME)
INSTALLBINDIR = $(INSTALLROOTDIR)/bin/Cygwin_NT
ESUNLIBDIR = $(INSTALLROOTDIR)/lib/Cygwin_NT
ESUNINCLUDEDIR = $(INSTALLROOTDIR)/include
CC = gcc
CFLAGS = -O3 -I$(ESUNINCLUDEDIR) -I$(ESUNINCLUDEDIR)/ix86_Unix_gcc2
LDFLAGS = -L$(ESUNLIBDIR)
LIBRARIES = $(ESUNLIBDIR)/libesun.a

include Makefile-Common

